<% if users.any? %>
  <ul>
    <% users.each do |user| %>
      <li>
        <div>
          <div>
            <%= user.name %>
          </div>
          
          <div>
            <p><%= link_to 'Profile', user_path(user) %></p>
          </div>
          
        </div>
      </li>
    <% end %>
  </ul>
<% end %>